<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find rikirobot_4wd)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find rikirobot_4wd)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find rikirobot_4wd)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find rikirobot_4wd)/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find rikirobot_4wd)/param/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find rikirobot_4wd)/param/move_base_params.yaml" command="load" />
  </node>
</launch>
